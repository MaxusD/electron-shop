{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://electron-shop-mwdd.onrender.com/api/$1"
    }
  ],
  "redirects": [
    { "source": "/admin", "destination": "/index.html", "statusCode": 200 },
    { "source": "/admin/(.*)", "destination": "/index.html", "statusCode": 200 }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}